{{# partial "titulo" }}
    <title>Reportes</title>
{{/partial}}

<head>
    <link rel="stylesheet" type="text/css" href="/reportes.css">
</head>

{{# partial "contenido" }}
    <section>
        Organizaciones
        <section class="container" style="border: ridge #141414 1px;">
            <table style="text-align:center;">
                <tr class="row-tr">
                    <td>Id</td>
                    <td>Nombre</td>
                    <td>HC</td>
                    <td>Clasificacion</td>
                </tr>
                {{#each organizacionVistas}}
                    <tr class="row-tr">
                        <td>{{id}}</td>
                        <td>{{nombre}}</td>
                        <td>{{hclegible}}</td>
                        <td>{{clasificacion}}</td>
                    </tr>
                {{/each}}
            </table>
            <div>
            Total HC: {{hcTotal}}
            </div>
       </section>
    </section>
{{/partial}}
        
{{> layout.html.hbs }}

 